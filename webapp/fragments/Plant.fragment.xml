<core:FragmentDefinition
		xmlns="sap.m"
		xmlns:core="sap.ui.core">
	<SelectDialog
		noDataText="{i18n>notfound}"
		title="{i18n>selectplant}"
		search="fnhandleSearchPlant"
		confirm="fnCloseDialogPlant"
		cancel="fnCloseDialogPlant"
		items="{
			path: 'plantModel>/PlantData'
		}" >
		<StandardListItem
			id="listPlant"
			title="{plantModel>Name1}"
			description="{plantModel>Werks}"
			iconDensityAware="false"
			iconInset="false"
			type="Active" />
	</SelectDialog>
</core:FragmentDefinition>